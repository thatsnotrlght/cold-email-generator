import chromadb

chroma_client = chromadb.Client()
collection = chroma_client.create_collection(name= 'test_collection')

collection.add(
    documents = [
        "This is a test document about nothing",
        "This is not a test document about everything"
    ],
    ids = ["id1", "id2"]
)

all_docs = collection.get()
print("All docs\n", all_docs)

documents = collection.get(ids=["id1"])
print(documents)